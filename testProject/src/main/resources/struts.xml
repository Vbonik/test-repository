<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

    <package name="default" namespace="/" extends="struts-default">
        <action name="Ftp" class="com.issoft.ftp.presentation.action.FtpAction">
            <result name="SUCCESS">jsp/welcomePage.jsp</result>
        </action>
        <action name="uploadFtp" method="upload" class="com.issoft.ftp.presentation.action.FtpAction">
            <result name="SUCCESS">welcomePage.jsp</result>
            <result name="FAILURE">warning.jsp</result>
        </action>
        <action name="downloadFtp" method="download" class="com.issoft.ftp.presentation.action.FtpAction">
            <result name="SUCCESS">welcomePage.jsp</result>
            <result name="FAILURE">warning.jsp</result>
        </action>
        <action name="loginFtp" method="login" class="com.issoft.ftp.presentation.action.FtpAction">
            <result name="SUCCESS">jsp/upload.jsp</result>
        </action>
        <action name="getDownloadFileListFtp" method="getDownloadFileList"
                class="com.issoft.ftp.presentation.action.FtpAction">
            <result name="SUCCESS">download.jsp</result>
            <result name="FAILURE">warning.jsp</result>
        </action>
    </package>

</struts>
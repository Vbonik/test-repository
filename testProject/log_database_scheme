
CREATE DATABASE `log`

CREATE TABLE `LOG_TABLE` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_name` varchar(45) DEFAULT NULL,
  `authorities` longtext,
  `action` longtext,
  `status` longtext,
  `date` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=36 DEFAULT CHARSET='cp1251'


